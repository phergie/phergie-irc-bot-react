{
    "name": "phergie/phergie-irc-bot-react",
    "description": "IRC bot built on React",
    "keywords": [ "irc", "bot", "react" ],
    "license": "BSD-2-Clause",
    "support": {
        "email": "team@phergie.org",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/phergie/phergie-irc-bot-react/issues",
        "source": "https://github.com/phergie/phergie-irc-bot-react"
    },
    "require": {
        "php": ">=5.6.0",
        "phergie/phergie-irc-client-react": "~3.1",
        "phergie/phergie-irc-connection": "~2.0",
        "phergie/phergie-irc-event": "~1.0",
        "evenement/evenement": "~2.0",
        "monolog/monolog": "~1.12"
    },
    "require-dev": {
        "phergie/phergie-irc-bot-react-development": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Phergie\\Irc\\Bot\\React\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phergie\\Irc\\Tests\\Bot\\React\\": "tests"
        }
    },
    "bin": [
        "bin/phergie"
    ]
}
